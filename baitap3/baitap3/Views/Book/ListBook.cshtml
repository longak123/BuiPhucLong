
@{
    Layout = null;
}
<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta name="viewport" content="width=device-width"/>
    <title>ListBook</title>
</head>
<body>
    <div>
        <ul>
            @{int i = 1; }
            @foreach(var item in ViewBag.Books)
            {
                <li>@i - <b>@item</b></li>
                i++;
            }
        </ul>
    </div>
</body>
</html>

